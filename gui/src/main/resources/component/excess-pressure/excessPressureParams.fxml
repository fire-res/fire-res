<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <padding>
        <Insets top="24.0"/>
    </padding>
    <TextField prefHeight="25.0" prefWidth="190.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <GridPane.margin>
            <Insets left="15.0" top="15.0"/>
        </GridPane.margin>
    </TextField>
    <TextField prefHeight="25.0" prefWidth="190.0" GridPane.columnIndex="1">
        <GridPane.margin>
            <Insets left="15.0"/>
        </GridPane.margin>
    </TextField>
    <Label prefHeight="8.0" prefWidth="137.0" text="Базовое давление:" wrapText="true">
        <GridPane.margin>
            <Insets left="15.0"/>
        </GridPane.margin>
    </Label>
    <Label prefHeight="0.0" prefWidth="92.0" text="Давление:" GridPane.rowIndex="1">
        <GridPane.margin>
            <Insets left="15.0" top="15.0"/>
        </GridPane.margin>
    </Label>
</GridPane>